<!DOCTYPE html>
<html>
  <head>
    <title>Mapflagandeagle</title>
    <%= csrf_meta_tags %>
    <%= csp_meta_tag %>

    <%= stylesheet_link_tag    'application', media: 'all' %>
    <%= javascript_include_tag 'application' %>
  </head>

  <body>
    <% if !current_user %>
      <%= link_to "Log In", google_oauth2_login_path %>
    <% else %>
      <%= current_user.name %>
      <%= link_to "Log Out", logout_path %>
    <% end %>
    <%= yield %>
  </body>
</html>
